{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"dev": "npm run migration:run && nest start --watch | pino-pretty -c",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest --detectOpenHandles --runInBand --forceExit",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles",
		"typeorm": "ts-node ./node_modules/typeorm/cli",
   	"migration:generate": "ts-node -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli.js -d ./src/utils/database/typeorm.config.ts migration:generate ./src/utils/database/migrations/$npm_config_name",
		"migration:run": "ts-node -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli.js -d ./src/utils/database/typeorm.config.ts migration:run",
		"migration:revert": "ts-node -r tsconfig-paths/register -r dotenv/config ./node_modules/typeorm/cli.js -d ./src/utils/database/typeorm.config.ts migration:revert",
    		"convert:typeorm": "yarn build && ts-node ./convert-typeorm.config.js",
		"db:diagram": "ts-node ./node_modules/.bin/typeorm-uml ormconfig.json --handwritten --download=uml.png",
		"build-swagger": "ts-node -r tsconfig-paths/register src/utils/generate-swagger.ts"
  },
  	"resolutions": {
		"string-width": "^4.2.3",
		"wrap-ansi": "^7.0.0",
		"cliui": "^7.0.4"
	},
  "dependencies": {
		"@keyv/redis": "^5.1.2",
		"@nestjs-modules/mailer": "^2.0.2",
		"@nestjs/bull": "^11.0.2",
		"@nestjs/cache-manager": "^3.0.1",
		"@nestjs/common": "^11.1.2",
		"@nestjs/config": "^4.0.2",
		"@nestjs/core": "^11.0.1",
		"@nestjs/jwt": "^11.0.0",
		"@nestjs/mongoose": "^11.0.3",
		"@nestjs/platform-express": "^11.0.1",
		"@nestjs/swagger": "^11.2.0",
		"@nestjs/throttler": "^6.4.0",
		"@nestjs/typeorm": "^11.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-metrics": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    		"cache-manager": "^7.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "csurf": "^1.11.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "nestjs-pino": "^4.4.0",
    "nodemailer": "^7.0.3",
    "pg": "^8.16.0",
		"pino": "^9.7.0",
		"pino-http": "^10.5.0",
		"pino-loki": "^2.6.0",
		"pino-pretty": "7",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.24",
    		"ulid": "^3.0.1"
  },
  "devDependencies": {
		"@eslint/eslintrc": "^3.2.0",
		"@eslint/js": "^9.18.0",
		"@nestjs/cli": "^11.0.0",
		"@nestjs/schematics": "^11.0.0",
		"@nestjs/testing": "^11.0.1",
		"@swc/cli": "^0.6.0",
		"@swc/core": "^1.10.7",
		"@types/bcrypt": "^5.0.2",
		"@types/express": "^5.0.0",
		"@types/jest": "^29.5.14",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/multer": "^2.0.0",
		"@types/node": "^22.10.7",
		"@types/nodemailer": "^6.4.17",
		"@types/supertest": "^6.0.2",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-prettier": "^5.2.2",
		"globals": "^16.0.0",
		"jest": "^29.7.0",
		"prettier": "^3.4.2",
		"source-map-support": "^0.5.21",
		"sqlite3": "^5.1.7",
		"supertest": "^7.0.0",
		"ts-jest": "^29.2.5",
		"ts-loader": "^9.5.2",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typeorm-uml": "^1.6.5",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
			"^@/(.*)$": "<rootDir>/src/$1",
			"^@users/(.*)$": "<rootDir>/src/users/$1",
			"^@brazilian_person/(.*)$": "<rootDir>/src/brazilian_person/$1",
			"^@estados/(.*)$": "<rootDir>/src/estados/$1",
			"^@cities/(.*)$": "<rootDir>/src/cities/$1",
			"^@mail/(.*)$": "<rootDir>/src/mail/$1",
			"^@app/(.*)$": "<rootDir>/src/app/$1",
			"^@config/(.*)$": "<rootDir>/src/config/$1"
    },
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"testEnvironment": "node",
		"coveragePathIgnorePatterns": [
			"/node_modules/",
			"/test/",
			"/dist/",
			"/convert-typeorm.config.js",
			"/src/database/migrations/",
			"/src/main.ts",
			"/src/tracing.ts"
		],
		"openHandlesTimeout": 5000,
		"coverageDirectory": "./test/coverage"
  }
}
